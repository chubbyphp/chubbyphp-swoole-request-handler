{
    "name": "chubbyphp/chubbyphp-swoole-request-handler",
    "description": "A request handler adaper for swoole, using PSR 7, PSR 15 and PSR 17.",
    "keywords": [
        "chubbyphp",
        "requesthandler",
        "swoole",
        "psr-7",
        "psr-15",
        "psr-17"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-swoole": "^4.4.6",
        "dflydev/fig-cookies": "^2.0",
        "psr/http-factory": "^1.0.1",
        "psr/http-message": "^1.0.1",
        "psr/http-server-handler": "^1.0.1"
    },
    "require-dev": {
        "blackfire/php-sdk": "^1.19.1",
        "chubbyphp/chubbyphp-mock": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.15.3",
        "infection/infection": "^0.14.1",
        "nunomaduro/phpinsights": "^1.9",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.3.5",
        "swoole/ide-helper": "^4.4.6"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\SwooleRequestHandler\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\SwooleRequestHandler\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
